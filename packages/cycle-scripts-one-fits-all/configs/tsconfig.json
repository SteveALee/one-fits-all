{
    "compileOnSave": false,
    "compilerOptions":
    {
        "module": "ES6",
        "moduleResolution": "node",
        "sourceMap": true,
        "jsx" : "preserve",
        "target": "ES6",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules"
    ]
}
